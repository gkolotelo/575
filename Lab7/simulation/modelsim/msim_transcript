# Loading std.textio(body)
# Loading ieee.std_logic_1164(body)
# Loading work.exp7_part2(behavior)
add wave -position end  sim:/exp7_part2/reset
force -freeze sim:/exp7_part2/reset 1 0 -cancel 10
run
run
