add wave -position end  sim:/exp7_part2/clock
force -freeze sim:/exp7_part2/clock 1 0, 0 {50 ps} -r 100
add wave -position end  sim:/exp7_part2/SW(0)
force -freeze sim:/exp7_part2/SW(0) 1 0, 0 {800 ps} -r 1000
run -all
